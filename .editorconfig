# editorconfig.org
root = true

[*]
quote_type = double
indent_style = tab
indent_size = 2
end_of_line = crlf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[{package.json,package-lock.json}]
indent_style = space
indent_size = 2
